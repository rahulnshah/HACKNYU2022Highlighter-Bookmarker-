{"name": "Highlight it!",
    "description": "Alows Students to highlight and save their page!",
        "manifest_version": 2,
        "name": "Highlight it!",
        "description": "Highlights website script and bookmarks script page.",
        "version": "1.0",
        "permissions": ["/images"],
        "icons": {
          "16": "/images/HackathonLogoV2.png",
          "48": "/images/HackathonLogoV2.png",
          "128": "/images/HackathonLogoV2.png"
        },
        "page_action": {
          "default_icon": {
            "16": "/images/HackathonLogoV2.png",
            "48": "/images/HackathonLogoV2.png",
            "128": "/images/HackathonLogoV2.png"
          }
        }
    }